{
    "compilerOptions": {
        "module": "es2015",
        "target": "es2015",
        "sourceMap": true,
        "allowUnreachableCode": false,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "strict": true
    },
    "exclude": [
        "node_modules"
    ]
}
